package it.unipr.informatica.examples;

public class ThreadB extends Thread {
	private int id;
	
	public ThreadB(int id) {
		if(id < 0)
			throw new IllegalArgumentException("id");
		
		this.id = id;
	}
	
	@Override
	public void run() {
		for(int i = 0; i < 10; ++i)
			System.out.println("A: " + i + " - id: " + id);
	}
}
